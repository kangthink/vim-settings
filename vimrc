let mapleader = "\<space>"

nnoremap <leader>w :wa<CR>
nnoremap <leader>q :wq<CR>
syntax enable
set tabstop=4
set softtabstop=4
set expandtab
set number
set showcmd
filetype indent on 
set showmatch 
set incsearch
set hlsearch
nnoremap j gj
nnoremap k gk
nnoremap E $
nnoremap B ^
nnoremap ^ <nop>
set tenc=utf-8
set fileencoding=utf-8
set shiftwidth=4

